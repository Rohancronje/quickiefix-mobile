{
  "expo": {
    "name": "QuickieFix",
    "slug": "quickiefix",
    "scheme": "quickiefix",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "nz.co.quickiefix.app",
      "infoPlist": {
        "CFBundleDisplayName": "QuickieFix"
      }
    },
    "android": {
      "package": "nz.co.quickiefix.app",
      "permissions": [
        "INTERNET"
      ]
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}